 <nav class="navbar navbar-dark bg-dark">
    <h2 class="text-white h2">Student List</h2>
    <a class="navbar-brand" href="/users/teacherdashboard"  style=" text-align: center; display: block;">
    Go to Dashboard
    </a>
</nav>
 <div class=" text-center" style="padding: 10%;">
        <div class="card-header">
 <ul class="nav nav-tabs" id="myTab" role="tablist">
  <li class="nav-item">
        <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Grade 1</a>
      </li>
      <li class="nav-item">
        <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Grade 6</a>
      </li>
    </ul>
</div>
<div class="">
    <div class="tab-content" id="myTabContent">
      <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
            <% Students.forEach(function(student) { %>
                <% if(student.grade == "1") { %>
                    <li class="list-group-item"><%= student.name %></li>
              <%  } %>
            <% }); %>

      </div>
      <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
            <% Students.forEach(function(student) { %>
                <% if(student.grade == "6") { %>
                    <li class="list-group-item"><%= student.name %></li>
              <%  } %>
            <% }); %>

      </div>
    </div>
</div>



